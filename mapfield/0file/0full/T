FoamFile                                                                           
{                                                                                  
    version     2.0;                                                               
    format      ascii;                                                             
    class       volScalarField;                                                    
    object      T;                                                                 
}                                                                                  
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //    
                                                                                   
dimensions             [0 0 0 1 0 0 0];                                            
                                                                                   
internalField          uniform 286.8591338974614; 
                                                                                   
boundaryField                                                                      
{                                                                                  
   inlet                                                                           
   {                                                                               
       type            fixedValue;                                                 
       value           $internalField;                                             
   }                                                                               
                                                                                   
   outlet                                                                          
   {                                                                               
       type            zeroGradient;                                               
   }                                                                               
                                                                                   
   wall                                                                            
   {                                                                               
       type            zeroGradient;                                               
   }                                                                               
                                                                                   
   "(top|bottom)"                                                                  
   {                                                                               
       type            slip;                                                       
   }                                                                               
                                                                                   
   #includeEtc "caseDicts/setConstraintTypes"                                      
}                                                                                  
                                                                                   
